{
  "name": "civil-registry-system",
  "version": "1.0.0",
  "description": "Complete Civil Registry Management System",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "npm run dev:frontend",
    "dev:frontend": "vite",
    "build": "vite build && cd server && npm run build",
    "start": "cd server && npm start",
    "db:setup": "cd server && npx prisma generate && npx prisma db push && npx prisma db seed",
    "db:migrate": "cd server && npx prisma migrate dev",
    "db:reset": "cd server && npx prisma migrate reset --force",
    "test": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.15.0",
    "react-i18next": "^13.2.2",
    "i18next": "^23.5.1",
    "@tanstack/react-query": "^4.32.6",
    "axios": "^1.5.0",
    "framer-motion": "^10.16.4",
    "react-hook-form": "^7.45.4",
    "@hookform/resolvers": "^3.3.1",
    "yup": "^1.3.2",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.8.0",
    "react-qr-code": "^2.0.11",
    "html5-qrcode": "^2.3.8",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.344.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "concurrently": "^8.2.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vitest": "^0.34.4"
  }
}